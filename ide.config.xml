<config>
	<wintitle>Smash</wintitle>
	<about>Smash 0.2. Programmed by Javi Cervera.</about>
	<filefilter>Smash files (*.smash);;All files (*.*)</filefilter>
	<defaultext>.nb</defaultext>
	<tabsize>3</tabsize>
	<font>
		<!--register>cfg/Envy Code R Bold.ttf</register-->
		<name:win>Courier New</name:win>
		<size:win>10</size:win>
		<name:mac>Monaco</name:mac>
		<size:mac>12</size:mac>
		<name:linux>Courier New</name:linux>
		<size:linux>12</size:linux>
		<flags bold="false" italic="false" underline="false" />
	</font>
	<colors>
		<background rgb="0x225588" />
		<foreground rgb="0xEEEEEE" />
		<comments rgb="0xFFEE00" />
		<strings rgb="0x00FF66" />
		<keywords rgb="0xAAFFFF" />
		<numbers rgb="0x33FFDD" />
		<preprocessor rgb="0xFFAA00" />
		<symbols rgb="0x008080" />
	</colors>
</config>